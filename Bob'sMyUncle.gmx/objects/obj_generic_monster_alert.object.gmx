            <string>// Enemey AI Dectection and Seeking

// check to see if player is in wide dectection area if not revert back to regular object/walking
if (distance_to_object(obj_player) &gt; aggroRange ) {
    isAggro = false;
    // check which object we are
    _objName = object_get_name(object_index);
    if (_objName == "obj_skeleton_alert") || (_objName == "obj_skeleton_attack") {
        instance_change(obj_skeleton, true);
    }
    else if (_objName == "obj_slime_alert") || (_objName == "obj_slime_attack") {
        instance_change(obj_slime, true);
    }
}
else {
    isAggro = true;
}

// check to see if we are in attack distance
if (distance_to_object(obj_player) &lt;= aggroRange) &amp;&amp; (distance_to_object(obj_player) &gt;= attackDistance) {
    // get player cords
    if (obj_player.x &lt;= x) {
        // move LEFT towards player
        hspeed = -2;
    }
    else{
        // flip default sprite on x axis. Mirror horizontal.
        image_xscale = -1;
        // move RIGHT towards player
        hspeed = 2;
    }
}
else {
    hspeed = 0;
    // get instance name
    _objName = object_get_name(object_index);
    if (_objName == "obj_skeleton_alert") {
        instance_change(obj_skeleton_attack, true);
        show_debug_message("obj skeleton attack created");
    }
    else if (_objName == "obj_slime_alert") {
        show_debug_message("obj slime attack created");
        instance_change(obj_slime_attack, true);
    }
}
</string>
